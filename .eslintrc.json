{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "semi": ["error", "never"],
        "comma-dangle": ["error", "never"],
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
}